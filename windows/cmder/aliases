e.=explorer .
gl=git log --oneline --all --graph --decorate  $*
ls=ls --color $*
pwd=cd
clear=cls

ga=git add -A
gc=git commit -m $*
gs=git status -s
gcb=git checkout -b $*
gpull=git pull --rebase --prune
aliases=cat C:\tools\cmder\config\aliases
oss=cd c:\git\oss\
rr=cd c:\git\rr\
apmu=apm upgrade -c=false
npml=npm list -g --depth=0
npmu=npm-check-updates $*
